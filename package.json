    "prettier:check": "prettier --check 'src/**/*.{jsx,js,ts,tsx}'",
    "tsc": "tsc",
    "ts": "node -r esm -r ts-node/register/transpile-only ",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "@apollo/client": "^3.4.17",
    "@chakra-ui/icons": "^1.1.5",
