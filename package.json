    "@contentful/rich-text-react-renderer": "^14.1.3",
    "@contentful/rich-text-types": "^14.1.2",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/montserrat": "^4.5.3",
    "classnames": "^2.2.6",
    "contentful": "^7.14.12",
    "date-fns": "^2.19.0",
    "fast-safe-stringify": "^2.0.8",
    "framer-motion": "^5",
    "graphql": "^16.0.1",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "next": "^12.0.1",
    "pigeon-maps": "^0.20.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-player": "^2.9.0",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.0",
    "@graphql-codegen/typescript-operations": "2.2.0",
    "@graphql-codegen/typescript-react-apollo": "^3.2.2",
    "@netlify/plugin-nextjs": "^4.3.2",
    "@types/classnames": "^2.3.1",
    "@types/lodash": "^4.14.171",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "autoprefixer": "^10.4.0",
    "cf-content-types-generator": "^1.5.2",
    "dotenv": "8.2.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "esm": "^3.2.25",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.4.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
